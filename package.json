{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder -mwl"
  },
  "repository": "https://github.com/jsobralgitpush/plan-farm.git",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Sobral <jose.sobral@poli.ufrj.br>",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^21.1.1",
    "electron-builder": "^22.10.5"
  },
  "dependencies": {
    "electron-reload": "^1.5.0",
    "imask": "^6.0.7",
    "jquery": "^3.5.1",
    "jsonfile": "^6.1.0"
  },
  "build": {
    "appId": "com.example.electron-boilerplate",
    "files": [
      "./**"
    ],
    "productName": "Gestao_Fazenda",
    "directories": {
      "buildResources": "resources",
      "output": "../out_farm"
    },
    "extraResources": [ {
      "from": "./db",
      "to": "../../db"
    }
    ],
    "publish": null
  }
}
